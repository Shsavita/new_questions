#include<bits/stdc++.h>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
string s;
cin>>s;
map<char,int>m;
for(int i=0;i<s.size();i++){
	m[s[i]]++;
}
bool f=false;
for(int i=0;i<s.size();i++){
	
	if(m[s[i]] == 1){
		f=true;
		cout<<s[i]<<endl;
		break;
	}
}
if(!f){
	cout<<-1<<endl;
}
	}
	return 0;
}
